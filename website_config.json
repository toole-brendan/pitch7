{
    "IndexDocument": {
        "Suffix": "index.html"
    },
    "ErrorDocument": {
        "Key": "index.html"
    },
    "RoutingRules": [
        {
            "Condition": {
                "KeyPrefixEquals": "pitch"
            },
            "Redirect": {
                "ReplaceKeyWith": "pitch/index.html"
            }
        }
    ]
}
